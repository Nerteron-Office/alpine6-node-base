# Common packages
common="curl make gcc g++ git python"

# Install packages to images docker
addalpineBase="$common dnsmasq"
addapplication="$common"
addcamel="$common"
addauthentication="$common cairo pango libjpeg jpeg-dev giflib giflib-dev ttf-liberation krb5-dev cairo-dev"
addcache="$common"
addappservice="$common"
addusergueststatuspollingservice="$common"
addsync="$common"
addcdn="$common krb5-dev"
addprincipalService="$common"
addserviceDiscovery="curl unzip python"
addhttprelay="$common krb5-dev"
addpresenceService="$common"
addwebsockify="$common"
addproxy="nginx"
addseafileServer=""

# Delete packages to images docker
delalpineBase="$common"
delapplication="$common"
delcamel="$common"
delauthentication="$common jpeg-dev giflib-dev krb5-dev cairo-dev"
delcache="$common"
delappservice="$common"
delusergueststatuspollingservice="$common"
delsync="$common"
delcdn="$common krb5-dev"
delprincipalService="$common"
delserviceDiscovery="curl unzip"
delhttprelay="$common krb5-dev"
delpresenceService="$common"
delwebsockify="$common"
delproxy=""
delseafileServer=""